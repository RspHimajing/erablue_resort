@CAN_体位(必要実行値)
#DIM 必要実行値
#DIM 実行値
#DIM ＋表示フラグ

SIF CFLAG:TARGET:睡眠
	RETURN 1

;-------------------------------------------------
;実行判定処理
;-------------------------------------------------
;縄で縛られている場合は無視（縄が無い時のみ実行判定を行う）
;IF TEQUIP:縄 == 0
	実行値 = 0
	＋表示フラグ = 0

	;すべての命令に共通の要素を考慮
	CALL COM_ORDER
	実行値 = RESULT
	＋表示フラグ = RESULT:1

	;ABL:欲望
	IF ABL:欲望
		SIF ＋表示フラグ
			PRINT  + 
		実行値 += ABL:欲望 * 3
		PRINTFORM 欲望LV{ABL:欲望}({ABL:欲望 * 3})
		＋表示フラグ = 1
	ENDIF
	IF RFLAG:指示モード対象あなたフラグ
		;ABL:Ｃ感覚
		IF ABL:Ｃ感覚
			SIF ＋表示フラグ
				PRINT  + 
			実行値 += ABL:Ｃ感覚 * 3
			PRINTFORM Ｃ感覚LV{ABL:Ｃ感覚}({ABL:Ｃ感覚 * 3})
			＋表示フラグ = 1
		ENDIF
	ELSE
		;ABL:Ｖ感覚
		IF ABL:Ｖ感覚
			SIF ＋表示フラグ
				PRINT  + 
			実行値 += ABL:Ｖ感覚 * 2
			PRINTFORM Ｖ感覚LV{ABL:Ｖ感覚}({ABL:Ｖ感覚 * 2})
