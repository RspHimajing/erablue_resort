@COMNAME60
#FUNCTIONS
TSTR:コマンド名受渡 = Ｖ挿入する
SIF MODE_存在判定_順不同("Ｖ挿入系", PLAYER, TARGET)
	TSTR:コマンド名受渡 = 体位を変える

;弱点コマンド欄に専用の文字列を表示したい場合、これを設定すること
弱点コマンド欄表示名 = Ｖ挿入する

SIF 派生名フラグ
	TSTR:コマンド名受渡 = Ｖ挿入

;許可時の特別文章はここに記入
IF TFLAG:オートコマンドフラグ >= 4000
	;反転時、つまりあなたが入れる方
	許可取り文章 = %CALLNAME:TARGET%は%CALLNAME:PLAYER%に[Ｖ挿入]して欲しいようだ…
ELSE
	許可取り文章 = %CALLNAME:PLAYER%は%CALLNAME:TARGET%に[Ｖ挿入]したいようだ…
ENDIF

;===================================================
;コマンドタイプ
;===================================================
@COMTYPE_60
#FUNCTION
TFLAG:コマンドタイプ受渡 = COMTYPE("Ｖ系")
SIF CFLAG:TARGET:睡眠
	TFLAG:コマンドタイプ受渡 = COMTYPE("セクハラ強")
睡眠中許可 = 1
;弱点コマンドカテゴリ = V
同時モード許可 = 3
同時モード拡張許可 = 1
オートコマンド許可 = 3
反転オート許可 = 3

@COM_TOOLTIP_60
LOCALS = <br>■Ｖ挿入する<br>
LOCALS += "相手の膣に挿入し、セックスを行うコマンド。<br>Ｖ挿入系モード時、[体位を変える]に変化する。<br>Ｖ挿入系モードとなり、モードを変更するか性行為を終了するまで持続的に快楽が発生する。<br>[処女]を失う。"
LOCALS += "<br>COMタイプ：Ｖ系<br>取得ソース：快Ｖ・露出・接触・苦痛・不潔(一定条件時)"
LOCALS += "<br>固有の取得経験：Ｖ性交経験(TARGET)・Ｃ性交経験_Ｖ挿入(PLAYER)<br>"
TSTR:ツールチップ受渡 = %LOCALS%

@COM60
#DIM ランダム候補, 20
#DIM 強さ初期値記録
#DIM 速さ初期値記録
#DIMS 体位名

IF TFLAG:オートコマンドフラグ
	;オートで実行する場合
	;ランダムで体位を決定して実行する
	VARSET ランダム候補, -1
	LOCAL:1 = 0
	FOR LOCAL, 0, 12
		TRYCALLFORM COM_ABLE60_{LOCAL}
		IF RESULT
			ランダム候補:(LOCAL:1) = LOCAL
			LOCAL:1 ++
		ENDIF
	NEXT
	LOCAL = ランダム候補:(RAND:(LOCAL:1))
	TRYCALLFORM COM60_{LOCAL}
	;メッセージ用に体位の種類を保存
	RFLAG:コマンド結果受渡し変数 = LOCAL
	CALL オート体位変更Ｖ
	RETURN 1
ELSEIF GETBIT(FLAG:モード管理, モードビット_同時)
	;同時挿入
	IF RFLAG:コマンド結果受渡し変数 == 12
		TRYCALLFORM COM60_12(強さ初期値記録, 速さ初期値記録)
	ELSE
		強さ初期値記録 = 0
		速さ初期値記録 = 0
		$体位_LOOP2
		DRAWLINE
		PRINTL ■速さ・強さの初期値を選んでください
		PRINT 　　Ｖ挿入強さ：
		PRINTBUTTON "[-]", 899
		FOR LOCAL, 0, 6
			IF 強さ初期値記録 >= LOCAL
				PRINTBUTTON "■", 900 + LOCAL
			ELSE
				PRINTBUTTON "□", 900 + LOCAL
			ENDIF
		NEXT
		PRINTBUTTON "[+]", 906
		PRINT 　　
		PRINT Ｖ挿入速さ：
		PRINTBUTTON "[-]", 909
		FOR LOCAL, 0, 6
			IF 速さ初期値記録 >= LOCAL
				PRINTBUTTON "■", 910 + LOCAL
			ELSE
				PRINTBUTTON "□", 910 + LOCAL
			ENDIF
		NEXT
		PRINTBUTTON "[+]", 916
		PRINTL
		PRINTL
		PRINTBUTTONLC "[998]決定", 998
		PRINTBUTTONLC "[999]戻る", 999

		BINPUT

		LOCAL = RESULT
		SELECTCASE LOCAL
			CASE 900 TO 905
				強さ初期値記録 = LOCAL - 900
				GOTO 体位_LOOP2
			CASE 910 TO 915
				速さ初期値記録 = LOCAL - 910
				GOTO 体位_LOOP2
			CASE 899
				強さ初期値記録 = MAX(強さ初期値記録 - 1, 0)
				GOTO 体位_LOOP2
			CASE 906
				強さ初期値記録 = MIN(強さ初期値記録 + 1, 5)
				GOTO 体位_LOOP2
			CASE 909
				速さ初期値記録 = MAX(速さ初期値記録 - 1, 0)
				GOTO 体位_LOOP2
			CASE 916
				速さ初期値記録 = MIN(速さ初期値記録 + 1, 5)
				GOTO 体位_LOOP2
			CASE 999
				RETURN -1
		ENDSELECT
		LOCAL:10 = TARGET
		FOR LOCAL, 0, 同時モード_選択数
			SIF 同時モード_選択キャラ:LOCAL < 1
				BREAK
			RESULT = 0
			TARGET = 同時モード_選択キャラ:LOCAL
			TRYCALLFORM COM_ABLE60_12
			IF RESULT == 0
				TARGET = LOCAL:10
				RETURN -1
			ENDIF
			RESULT = 1
			TRYCALLFORM CAN_体位12(1)
			IF !RESULT
				TRYCALLFORM CAN_体位12(0)
				RETURN -1
			ENDIF
		NEXT
		TARGET = LOCAL:10

		TRYCALLFORM COM60_12(強さ初期値記録, 速さ初期値記録)
		;メッセージ用に体位の種類を保存
		RFLAG:コマンド結果受渡し変数 = 12
	ENDIF
	RETURN 1
ELSE
	IF MODE_存在判定_完全一致("Ｖ挿入系", PLAYER, TARGET)
		強さ初期値記録 = V挿入強さ
		速さ初期値記録 = V挿入速さ
	ELSE
		強さ初期値記録 = 0
		速さ初期値記録 = 0
	ENDIF
	$体位_LOOP
	DRAWLINE
	PRINTL ■体位を選んでください
	PRINTL
	LOCAL:1 = 0
	FOR LOCAL, 0, 20
		TRYCCALLFORM COM_ABLE60_{LOCAL}
			SELECTCASE LOCAL
				CASE 0
					体位名 = 正常位
				CASE 1
					体位名 = 後背位
				CASE 2
					体位名 = 寝バック
				CASE 3
					体位名 = 対面座位
				CASE 4
					体位名 = 背面座位
				CASE 5
					体位名 = 騎乗位
				CASE 6
					体位名 = 側位
				CASE 7
					体位名 = 屈曲位
				CASE 8
					体位名 = 駅弁
				CASE 9
					体位名 = 背面駅弁
				CASE 10
					体位名 = 対面立位
				CASE 11
					体位名 = 立ちバック
				CASEELSE
					CONTINUE
			ENDSELECT
			TRYCALLFORM 固有Ｖ体位ABLE_プレイヤー側%体位名%_{NO:PLAYER}(PLAYER)
			TRYCALLFORM 固有Ｖ体位ABLE_ターゲット側%体位名%_{NO:TARGET}(TARGET)
			IF RESULT
				LOCAL:1 ++
				PRINTBUTTONLC @"[{LOCAL}]%体位名%", LOCAL
			ENDIF
		CATCH
			CONTINUE
		ENDCATCH
	NEXT
	IF !LOCAL:1
		PRINTW 現在選べる体位がないのでキャンセルします
		RETURN -1
	ENDIF

	PRINTL
	PRINTL
	PRINTL ■速さ・強さの初期値を選んでください
	PRINT 　　Ｖ挿入強さ：
	PRINTBUTTON "[-]", 899
	FOR LOCAL, 0, 6
		IF 強さ初期値記録 >= LOCAL
			PRINTBUTTON "■", 900 + LOCAL
		ELSE
			PRINTBUTTON "□", 900 + LOCAL
		ENDIF
	NEXT
	PRINTBUTTON "[+]", 906
	PRINT 　　
	PRINT Ｖ挿入速さ：
	PRINTBUTTON "[-]", 909
	FOR LOCAL, 0, 6
		IF 速さ初期値記録 >= LOCAL
			PRINTBUTTON "■", 910 + LOCAL
		ELSE
			PRINTBUTTON "□", 910 + LOCAL
		ENDIF
	NEXT
	PRINTBUTTON "[+]", 916
	PRINTL
	PRINTL
	PRINTBUTTONLC "[999]戻る", 999

	$INPUT_LOOP
	BINPUT

	LOCAL = RESULT
	SELECTCASE LOCAL
		CASE 0 TO 11
			TRYCALLFORM COM_ABLE60_{LOCAL}
			IF RESULT
				IF TARGET == MASTER
					TARGET = PLAYER
					PLAYER = MASTER
					RFLAG:指示モード対象あなたフラグ = 1
					TRYCALLFORM CAN_体位{LOCAL}(1)
					IF !RESULT
						TRYCALLFORM CAN_体位{LOCAL}(0)
						PLAYER = TARGET
						TARGET = MASTER
						GOTO 体位_LOOP
					ENDIF
					RFLAG:指示モード対象あなたフラグ = 0
					PLAYER = TARGET
					TARGET = MASTER
				ELSE
					TRYCALLFORM CAN_体位{LOCAL}(1)
					IF !RESULT
						TRYCALLFORM CAN_体位{LOCAL}(0)
						GOTO 体位_LOOP
					ENDIF
				ENDIF
				TRYCALLFORM COM60_{LOCAL}(強さ初期値記録, 速さ初期値記録)
				;メッセージ用に体位の種類を保存
				RFLAG:コマンド結果受渡し変数 = LOCAL
				
				CALL オート体位変更Ｖ
				RETURN 1
			ENDIF
			GOTO INPUT_LOOP
		CASE 900 TO 905
			強さ初期値記録 = LOCAL - 900
			GOTO 体位_LOOP
		CASE 910 TO 915
			速さ初期値記録 = LOCAL - 910
			GOTO 体位_LOOP
		CASE 899
			強さ初期値記録 = MAX(強さ初期値記録 - 1, 0)
			GOTO 体位_LOOP
		CASE 906
			強さ初期値記録 = MIN(強さ初期値記録 + 1, 5)
			GOTO 体位_LOOP
		CASE 909
			速さ初期値記録 = MAX(速さ初期値記録 - 1, 0)
			GOTO 体位_LOOP
		CASE 916
			速さ初期値記録 = MIN(速さ初期値記録 + 1, 5)
			GOTO 体位_LOOP
		CASE 999
			V挿入強さ = 強さ初期値記録
			V挿入速さ = 速さ初期値記録
			RETURN -1
		CASEELSE
			GOTO INPUT_LOOP
	ENDSELECT
ENDIF

@オート体位変更Ｖ
;Ａ挿入中
IF MODE_存在判定_ターゲット側("Ａ挿入系", TARGET)
	LOCAL = DT_CELL_GET("体位モードデータベース", モードID受渡し, "実行キャラ", 1)
	SELECTCASE RFLAG:コマンド結果受渡し変数
		;正常位
		CASE 0
			CALL MODE_CHANGE("Ａ騎乗位", LOCAL, TARGET)
		;対面座位
		CASE 3
			CALL MODE_CHANGE("Ａ背面座位", LOCAL, TARGET)
		;騎乗位
		CASE 5
			CALL MODE_CHANGE("Ａ後背位", LOCAL, TARGET)
		;駅弁
		CASE 8
			CALL MODE_CHANGE("Ａ立ちバック", LOCAL, TARGET)
		;対面立位
		CASE 10
			CALL MODE_CHANGE("Ａ背面駅弁", LOCAL, TARGET)
	ENDSELECT
ENDIF


