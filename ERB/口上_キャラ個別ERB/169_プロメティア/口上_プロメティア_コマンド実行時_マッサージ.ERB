	IF TALENT:対象キャラ:妊娠 && PREGNANCY_SIZE(対象キャラ) >= 2
		SELECTCASE RAND:4
		CASE 0 
			KSTR:(K++) = IMAGEPATH_%CSTR:対象キャラ:画像フォルダ%/口上表示用_妊娠エロマッサージ_マッサージ終了
		CASE 1
			KSTR:(K++) = IMAGEPATH_%CSTR:対象キャラ:画像フォルダ%/口上表示用_妊娠エロマッサージ_マッサージ終了1
		CASE 2
			KSTR:(K++) = IMAGEPATH_%CSTR:対象キャラ:画像フォルダ%/口上表示用_妊娠エロマッサージ_マッサージ終了2
		CASE 2
			KSTR:(K++) = IMAGEPATH_%CSTR:対象キャラ:画像フォルダ%/口上表示用_妊娠エロマッサージ_マッサージ終了3
		ENDSELECT
	ELSE
		SELECTCASE RAND:4
		CASE 0 
			KSTR:(K++) = IMAGEPATH_%CSTR:対象キャラ:画像フォルダ%/口上表示用_エロマッサージ_マッサージ終了
		CASE 1
			KSTR:(K++) = IMAGEPATH_%CSTR:対象キャラ:画像フォルダ%/口上表示用_エロマッサージ_マッサージ終了1
		CASE 2
			KSTR:(K++) = IMAGEPATH_%CSTR:対象キャラ:画像フォルダ%/口上表示用_エロマッサージ_マッサージ終了2
