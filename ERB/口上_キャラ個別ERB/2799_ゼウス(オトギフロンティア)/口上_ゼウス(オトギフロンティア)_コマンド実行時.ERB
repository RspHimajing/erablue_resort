;--------------------------------------------------
;RFLAG:コマンド結果受渡し変数
	;0:通常成功  1:大成功  2:特殊反応

@KOJO_COM_2799_314(対象キャラ)
#DIM 対象キャラ
CALL 口上変数初期化
SELECTCASE RFLAG:コマンド結果受渡し変数
	CASE 0
		;通常成功
		KSTR:(K++) = 「わぁ、プレゼントですか！ありがとうございます、%CALLNAME:PLAYER%さん！」
	CASE 1
		;大成功
		KSTR:(K++) = 「あ、これ……欲しかったやつだ……！」
		KSTR:(K++) = 「%CALLNAME:PLAYER%さん、ありがとうございます。わたし、大切にしますね！」
	CASE 2
		;特殊反応
		IF RSTR:コマンド結果受渡し文字列 == "任意のアイテム名"
			;恋慕。（あーあーマイクテスト、マイクテスト。特異点と器の接続が拡大されたのを確認。これが愛……）
			IF TALENT:TARGET:恋慕
				KSTR:(K++) = 「ありがとうございます、%CALLNAME:PLAYER%さん！」
				KSTR:(K++) = 「今度、%CALLNAME:PLAYER%さんにお返ししますね_H_」
			ELSE
				KSTR:(K++) = 「%CALLNAME:PLAYER%さん、わたしにプレゼントをあげるなんて、いい人なんですね！」
				KSTR:(K++) = 「わたし、人間さんのこと、気に入りました！」
			ENDIF
		ENDIF
ENDSELECT

CALL 口上変数表示(対象キャラ, 0)

;--------------------------------------------------
;COM351_連れ出す
;--------------------------------------------------
;RFLAG:コマンド結果受渡し変数が１だと手を繋ぐ方

@KOJO_COM_2799_351(対象キャラ)
#DIM 対象キャラ
CALL 口上変数初期化

IF RFLAG:コマンド結果受渡し変数
	IF TALENT:TARGET:恋慕
		;恋人繋ぎ
		IF GET_TARGETNUM() > 1
			KSTR:(K++) = 「えへへ、こうしてると、何だか、お似合いのカップルみたいですね……_H_」
		ELSE
			KSTR:(K++) = 「あ……_H_」
			KSTR:(K++) = 「わたしも、%CALLNAME:PLAYER%さんと手を繋ぎたかったところなんです_H_」
	ENDIF
		;手をつなぐ
		KSTR:(K++) = 「%CALLNAME:PLAYER%さんの手、暖かいですね」
	ENDIF
ELSE
	;連れ出す
	IF TALENT:TARGET:恋慕
		KSTR:(K++) = 「ん？デートですか？」
		KSTR:(K++) = 「どこに連れて行ってくれるのかなぁ～、楽しみです！」
	ELSE
		KSTR:(K++) = 「うん？　どうかしましたか？」
	ENDIF
ENDIF

CALL 口上変数表示(対象キャラ, 0)

